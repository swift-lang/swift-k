	<target name="deps">
		<ant antfile="${main.buildfile}" target="dep">
			<property name="module" value="abstraction"/>
		</ant>
		<ant antfile="${main.buildfile}" target="dep">
			<property name="module" value="resources"/>
		</ant>
		<ant antfile="${main.buildfile}" target="dep">
			<property name="module" value="util"/>
