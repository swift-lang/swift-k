<?xml version="1.0" encoding="UTF-8"?>
<jndiConfig xmlns="http://wsrf.globus.org/jndi/config">


    <!-- Configuration for Managed Job Services -->

    <service name="ManagedExecutableJobService">
        <resource
            name="home"
            type="org.globus.exec.service.exec.ManagedExecutableJobHome">
            <resourceParams>
                <parameter>
                    <name>
                        factory
                    </name>
                    <value>
                        org.globus.wsrf.jndi.BeanFactory
                    </value>
                </parameter>
                <parameter>
                    <name>resourceClass</name>
                    <value>org.globus.exec.service.exec.PersistentManagedExecutableJobResource</value>
                </parameter>
                <parameter>
                    <name>resourceKeyType</name>
                    <value>java.lang.String</value>
                </parameter>
            </resourceParams>
        </resource>
    </service>       

    <service name="ManagedMultiJobService">
        <resource
            name="home"
            type="org.globus.exec.service.multi.ManagedMultiJobHome">
            <resourceParams>
                <parameter>
                    <name>
                        factory
                    </name>
                    <value>
                        org.globus.wsrf.jndi.BeanFactory
                    </value>
                </parameter>
                <parameter>
                    <name>resourceClass</name>
                    <value>org.globus.exec.service.multi.PersistentManagedMultiJobResource</value>
                </parameter>
                <parameter>
                    <name>resourceKeyType</name>
                    <value>java.lang.String</value>
                </parameter>
            </resourceParams>
        </resource>
    </service>       
    <!-- Common Configuration for Managed Job *Factory* Service and Home -->

    <service name="ManagedJobFactoryService">
        <resource
            name="configuration"
            type="org.globus.exec.service.factory.FactoryServiceConfiguration">
            <resourceParams>
                <parameter>
                    <name>
                        factory
                    </name>
                    <value>
                        org.globus.wsrf.jndi.BeanFactory
                    </value>
                </parameter>
                <parameter>
                    <name>
                       managedExecutableJobServicePath
                    </name>
                    <value>
                       ManagedExecutableJobService
                   </value>
                </parameter>
                <parameter>
                    <name>
                       managedMultiJobServicePath
                    </name>
                    <value>
                       ManagedMultiJobService
                   </value>
                </parameter>
            </resourceParams>
        </resource>
        <resource
            name="home"
            type="org.globus.exec.service.factory.ManagedJobFactoryHome">
            <resourceParams>
                <parameter>
                    <name>
                        factory
                    </name>
                    <value>
                        org.globus.wsrf.jndi.BeanFactory
                    </value>
                </parameter>
                <parameter>
                    <name>resourceClass</name>
                    <value>org.globus.exec.service.factory.ManagedJobFactoryResource</value>
                </parameter>
                <parameter>
                    <name>resourceKeyType</name>
                    <value>java.lang.String</value>
                </parameter>
            </resourceParams>
        </resource>
        <resource
            name="homeConfiguration"
            type="org.globus.exec.service.factory.FactoryHomeConfiguration">            
            <resourceParams>
                <parameter>
                    <name>
                        factory
                    </name>
                    <value>
                        org.globus.wsrf.jndi.BeanFactory
                    </value>
                </parameter>
                <parameter>
                    <name>
                       globusLocation
                    </name>
                    <value>
                       /home/hategan/gt3.9.5-install
                   </value>
                </parameter>               
                <parameter>
                    <name>
                       x509CertDir
                    </name>
                    <value>
                       /etc/grid-security/certificates
                   </value>
                </parameter>
                <parameter>
                    <name>
                       hostCpuType
                    </name>
                    <value>
                       i686
                   </value>
                </parameter>
                <parameter>
                    <name>
                       hostManufacturer
                    </name>
                    <value>
                       pc
                   </value>
                </parameter>
                <parameter>
                    <name>
                        hostOsName
                    </name>
                    <value>
                        Linux
                   </value>
                </parameter>                       
                <parameter>
                    <name>
                        hostOsVersion
                    </name>
                    <value>
                        2.4.25
                   </value>
                </parameter>
                <parameter>
                    <name>
                        condorArchitecture
                    </name>
                    <value>
                        INTEL
                   </value>
                </parameter>            
                <parameter>
                    <name>
                        condorOS
                    </name>
                    <value>
                        LINUX
                    </value>
                </parameter>                 
                <parameter>
                    <name>
                        tcpPortRange
                    </name>
                    <value>
                        
                    </value>
                </parameter>
                <parameter>
                    <name>
                        stagingProtocol
                    </name>
                    <value>
                        https
                    </value>
                </parameter>
                <parameter>
                    <name>
                        stagingHost
                    </name>
                    <value>
                        plussed.mcs.anl.gov
                    </value>
                </parameter>
                <parameter>
                    <name>
                        stagingPort
                    </name>
                    <value>
                        8443
                    </value>
                </parameter>
                <parameter>
                    <name>
                        stagingSubject
                    </name>
                    <value>
                        
                    </value>
                </parameter>
                <parameter>
                    <name>
                        runQueueThreadCount
                    </name>
                    <value>
                        16
                    </value>
                </parameter>
                <parameter>
                    <name>
                        maxUnsubmittedJobCount
                    </name>
                    <value>
                        -1
                    </value>
                </parameter>
                <parameter>
                    <name>
                        maxPendingRequestCount
                    </name>
                    <value>
                        -1
                    </value>
                </parameter>
                <parameter>
                    <name>
                        maxSubmissionRetryCount
                    </name>
                    <value>
                        -1
                    </value>
                </parameter>
                <parameter>
                    <name>
                        submissionRetryDelay
                    </name>
                    <value>
                        -1
                    </value>
                </parameter>
            </resourceParams>
        </resource>

        <resource name="mdsConfiguration"
                  type="org.globus.wsrf.impl.servicegroup.client.MDSConfiguration">
            <resourceParams>
            <parameter> <name>reg</name>
                        <value>false</value>
            </parameter>
            <parameter> <name>factory</name>
                        <value>org.globus.wsrf.jndi.BeanFactory</value>
            </parameter>
            </resourceParams>
        </resource>
        

        <!-- LRMs are configured in another JNDI addition-->            
        
    </service>


</jndiConfig>
